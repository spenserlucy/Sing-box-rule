{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": false
  },
"experimental": {
    "cache_file": {
        "enabled": true,
        "path": "cache.db",
        "cache_id": "ctc"
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://ghp.ci/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "Rule"
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "address_strategy": "prefer_ipv4",
        "detour": "Proxy"
      },
      {
        "tag": "fallback",
        "address": "8.8.8.8",
        "address_resolver": "google",
        "address_strategy": "prefer_ipv4",
        "detour": "Proxy"
      },
      {
        "tag": "local-dns",
        "address": "223.5.5.5",
        "address_strategy": "prefer_ipv4",
        "detour": "direct"
      },
      {
        "tag": "fakeip-dns",
        "address": "fakeip"
      },
      {
        "tag": "block-dns",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "local-dns",
         "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "local-dns"
      },
      {
        "clash_mode": "Global",
        "server": "google"
      },
      {
         "domain_keyword": "ntp",
         "server": "local-dns"
      },
      {
        "rule_set": [
          "Binance",
          "OKX",
          "PayPal",
          "OpenAI",
          "Gemini",
          "GoogleVoice",
          "GoogleSearch",
          "geosite-gfw",
          "YouTube",
          "geosite-geolocation-!cn"
        ],
        "server": "fakeip-dns"
      },
      {
        "rule_set": [
          "ChinaMax",
          "geosite-cn",
          "geosite-category-games@cn",
          "geosite-geolocation-cn",
          "geosite-private",
          "geosite-apple@cn",
          "geoip-cn"
        ],
        "server": "local-dns"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "geosite-geolocation-!cn",
        "server": "google"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "google"
      }
    ],
    "final": "local-dns",
    "reverse_mapping": true,
    "independent_cache": true,
    "fakeip": {
        "enabled": true,
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
    }
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "address": [
        "172.19.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "mtu": 9000,
      "stack": "system",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "platform": {
        "http_proxy": {
          "enabled": false,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
    {
      "tag": "mixed",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true,
      "domain_strategy": "prefer_ipv4"
    }
  ],
  "route": {
    "rules": [
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "Proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "outbound": "block"
      },
      {
        "rule_set":  [
          "AWAvenue-Ads-Rule",
          "anti-ad-sing-box",
          "AdGuardSDNSFilter"
        ],
        "outbound": "block"
      },
      {
        "rule_set": [
          "Binance",
          "OKX"
        ],
        "outbound": "BinOKX"
      },
      {
        "rule_set": [
          "YouTube"
        ],
        "outbound": "Video"
      },
      {
        "rule_set": [
          "PayPal",
          "OpenAI",
          "Gemini",
          "GoogleVoice",
          "GoogleSearch"
        ],
        "outbound": "USRule"
      },
      {
        "rule_set": [
          "ChinaMax",
          "geosite-cn",
          "geosite-category-games@cn",
          "geosite-private",
          "geosite-apple@cn"
        ],
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn"
          },
          {
            "rule_set": "geoip-cn",
            "invert": true
          }
        ],
        "outbound": "Proxy"
      }
    ],
    "rule_set": [
      {

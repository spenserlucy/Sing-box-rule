{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": false
  },
"experimental": {
    "cache_file": {
        "enabled": true,
        "path": "cache.db",
        "cache_id": "ctc"
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "Smart",
      "default_mode": "Rule"
    }
  },
  "dns": {
     "servers": [
     {
        "type": "local", 
        "tag": "local" 
      },
      {
        "tag": "doh-cf",
        "server": "d.adguard-dns.com",
        "type": "https",
        "server_port": 443,
        "tls": {
          "enabled": true,
          "server_name": "d.adguard-dns.com"
        },
        "detour": "Smart",
        "path": "/dns-query/c131f748"
      }
    ],
    "rules": 
    "final": "doh-cf"
  },
  "ntp": {
    "enabled": true,
    "server": "time.apple.com",
    "server_port": 123,
    "interval": "30m0s",
    "detour": "Smart"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": ["172.19.0.1/30"],
      "auto_route": true,
      "mtu": 1500,
      "stack": "system",
      "strict_route": true,
      "route_exclude_address_set": ["geoip-cn"]
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "udp-cf"
    },
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
       "action": "hijack-dns"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "ip_is_private": true
          },
          {
            "domain": "geosite-cn"
          }
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "ChinaMax",
          "geosite-cn",
          "geosite-category-games@cn",
          "geosite-private",
          "geosite-apple@cn"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "Smart"
      },
      {
        "rule_set": [
          "YouTube"
        ],
        "outbound": "Video"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn"
          },
          {
            "rule_set": "geoip-cn",
            "invert": true
          }
        ],
        "outbound": "Smart"
      }
    ],
    "rule_set": [ 
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "YouTube",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/YouTube/YouTube.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "ChinaMax",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/shangguanhongxin/for-sing-box-and-surge/master/sing-box/ChinaMax/ChinaMax_All.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geosite-cn",
        "update_interval": "1d",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geosite-category-games@cn",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-category-games@cn.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geosite-private",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-private.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geosite-apple@cn",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-apple@cn.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geoip-cn",
        "update_interval": "1d",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs"
      },
      {
        "type": "remote",
        "format": "binary",
        "download_detour": "Smart",
        "tag": "geosite-geolocation-!cn",
        "update_interval": "1d",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs"
      },
      {
        "tag": "geosite-geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/refs/heads/rule-set/geosite-geolocation-cn.srs",
        "update_interval": "1d",
        "download_detour": "Smart"
      }
    ],
    "auto_detect_interface": false,
    "final": "Smart"
 },
